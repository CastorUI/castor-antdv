{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "@castor-ui/castor-antdv",
  "version": "1.0.4",
  "description": "基于ant-design-vue的vue3组件库",
  "homepage": "https://castorui.github.io/castor-antdv-help/",
  "repository": {
    "type": "git",
    "url": "https://github.com/CastorUI/castor-antdv"
  },
  "publishConfig": {
    "access": "public"
  },
  "main": "es/index.js",
  "module": "es/index.js",
  "types": "es/types/castor-antd.d.ts",
  "typings": "es/types/castor-antd.d.ts",
  "files": [
    "es"
  ],
  "scripts": {
    "docs:dev": "vitepress dev docs",
    "docs:build": "npm run build && vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "build": "vite build",
    "format": "prettier --write packages/,docs/demos/",
    "npmjs": "npm run build && npm publish --registry=https://registry.npmjs.org/"
  },
  "keywords": [
    "vue3",
    "ant-design-vue",
    "vitepress"
  ],
  "author": "markhe",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.14.2",
    "@vitejs/plugin-vue-jsx": "^4.0.0",
    "fs-extra": "^11.2.0",
    "sass": "1.59.3",
    "vite-plugin-dts": "^4.5.4",
    "vitepress": "^1.2.2"
  },
  "dependencies": {
    "@ant-design/icons-vue": "^6.1.0",
    "@vitepress-demo-preview/component": "^2.3.2",
    "@vitepress-demo-preview/plugin": "^1.2.3",
    "ant-design-vue": "4.2.6"
  }
}
